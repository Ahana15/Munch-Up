      }
      .content .colorchip {
        display: inline-block;
        margin-right: 30px;
      }
      .content .color_tag,
      .content .color_block {
        display: inline-block;
        vertical-align: middle;
      }
      .content .color_tag {
        width: 60px;
      }
      .content .color_block {
        border: solid 2px;
        width: 30px;
        height: 30px;
      }
      .content .color_block_yellow {
        background-color: #ffb336;
      }
      .content .color_block_blue {
